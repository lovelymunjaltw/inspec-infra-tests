# to build docker image from default dockerfile 
docker build . -t demo_image

# to run docker container  from docker image - demo_image
docker run -it --name demo_container demo_image:latest 


# to build docker image from another dockerfile 

docker build . -f Dockerfile_test -t vodqa:latest2


# command to find open process and port 
sudo lsof -i -n -P | grep TCP

